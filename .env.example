# Core app config
SKYGENT_SERVICE=https://bsky.social
SKYGENT_STORE_ROOT=~/.skygent
SKYGENT_OUTPUT_FORMAT=ndjson
SKYGENT_IDENTIFIER=

# Credentials (do not commit real values)
SKYGENT_PASSWORD=
SKYGENT_CREDENTIALS_KEY=

# Bluesky rate limiting / retries
SKYGENT_BSKY_RATE_LIMIT=250 millis
SKYGENT_BSKY_RETRY_BASE=250 millis
SKYGENT_BSKY_RETRY_MAX=5

# Jetstream profile resolution (request batching + cache)
SKYGENT_PROFILE_BATCH_SIZE=25
SKYGENT_PROFILE_CACHE_CAPACITY=5000
SKYGENT_PROFILE_CACHE_TTL=6 hours

# Resource monitoring (warnings only; set to 0 to disable)
SKYGENT_RESOURCE_CHECK_INTERVAL=60 seconds
SKYGENT_RESOURCE_STORE_WARN_BYTES=1073741824
SKYGENT_RESOURCE_RSS_WARN_BYTES=1073741824

# Sync settings
SKYGENT_SYNC_CONCURRENCY=5
SKYGENT_SYNC_CHECKPOINT_EVERY=100
SKYGENT_SYNC_CHECKPOINT_INTERVAL_MS=5000

# LLM general settings
SKYGENT_LLM_STRATEGY=auto
SKYGENT_LLM_BATCH_SIZE=16
SKYGENT_LLM_TEMPERATURE=0.2
SKYGENT_LLM_CACHE_CAPACITY=0
SKYGENT_LLM_CACHE_TTL=10 minutes
SKYGENT_LLM_PERSIST_CACHE_TTL=24 hours
SKYGENT_LLM_SYSTEM_PROMPT=
SKYGENT_LLM_BATCH_SYSTEM_PROMPT=

# LLM provider selection (ordered fallbacks)
SKYGENT_LLM_PROVIDERS=openai,anthropic,google
SKYGENT_LLM_MODEL=
SKYGENT_LLM_ATTEMPTS=1
SKYGENT_LLM_RETRY_DELAY=1 seconds

# OpenAI
SKYGENT_OPENAI_API_KEY=
SKYGENT_OPENAI_API_URL=
SKYGENT_OPENAI_ORG_ID=
SKYGENT_OPENAI_PROJECT_ID=
SKYGENT_OPENAI_MODEL=

# Anthropic
SKYGENT_ANTHROPIC_API_KEY=
SKYGENT_ANTHROPIC_API_URL=
SKYGENT_ANTHROPIC_API_VERSION=
SKYGENT_ANTHROPIC_MODEL=

# Google
SKYGENT_GOOGLE_API_KEY=
SKYGENT_GOOGLE_API_URL=
SKYGENT_GOOGLE_MODEL=
