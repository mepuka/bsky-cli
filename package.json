{
  "name": "skygent-bsky",
  "version": "0.1.0",
  "description": "Composable Bluesky data filtering and monitoring CLI built with Effect",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "bin": {
    "skygent": "./index.ts"
  },
  "scripts": {
    "prepare": "effect-language-service patch",
    "build": "tsc --build",
    "build:clean": "rm -rf dist .tsbuildinfo && bun run build",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "typecheck": "tsc --noEmit",
    "changeset": "changeset",
    "version": "changeset version",
    "release": "bun run build && changeset publish"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.8",
    "@effect/language-service": "^0.72.0",
    "@effect/vitest": "^0.27.0",
    "@types/bun": "latest",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@atproto/api": "^0.18.17",
    "@effect/ai": "^0.33.2",
    "@effect/ai-anthropic": "^0.23.0",
    "@effect/ai-google": "^0.12.1",
    "@effect/ai-openai": "^0.37.2",
    "@effect/cli": "^0.73.1",
    "@effect/platform": "^0.94.2",
    "@effect/platform-bun": "^0.87.1",
    "@effect/printer": "^0.47.0",
    "@effect/printer-ansi": "^0.47.0",
    "@effect/typeclass": "^0.38.0",
    "effect": "^3.19.15",
    "effect-jetstream": "^1.0.3"
  }
}
